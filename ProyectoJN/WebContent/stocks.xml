<?xml version="1.0" encoding="UTF-8"?>
<proyectojn>
  <% while (folio.hasNext()) { %>
    <% stock = (Stock)folio.next(); %>
    <stock>
      <symbol><%=bean.getIdAlumno()%></symbol>
      <name<>%= 
      stock.getName() %></name>
      <price<>%= 
      stock.getPrice() %></price>
    </stock>
  <% } %>
</proyectojn>